{
  "name": "kollavarsham",
  "version": "1.0.0",
  "description": "Convert Gregorian date to Kollavarsham date and vice versa",
  "homepage": "http://kollavarsham.org",
  "repository": {
    "type": "git",
    "url": "kollavarsham/kollavarsham-js"
  },
  "author": {
    "name": "The Kollavarsham Team",
    "email": "info@kollavarsham.org",
    "url": "https://kollavarsham.org"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "engineStrict": true,
  "engines": {
    "node": ">= 6.0.0"
  },
  "keywords": [
    "kollavarsham",
    "malayalam",
    "calendar",
    "malayalam era",
    "gregorian calendar",
    "hindu calendar",
    "solar calendar",
    "lunar calendar",
    "sidereal calendar",
    "chingam",
    "kanni",
    "thulam",
    "vrischikam",
    "dhanu",
    "makaram",
    "kumbham",
    "meenam",
    "medam",
    "idavam",
    "mithunam",
    "karkitakam"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "eslint": "^4.10.0",
    "eslint-config-xo-space": "^0.17.0",
    "eslint-plugin-babel": "^4.1.2",
    "grunt": "^1.0.1",
    "grunt-contrib-yuidoc": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-coveralls": "^0.1.4",
    "gulp-eslint": "^4.0.0",
    "gulp-exclude-gitignore": "^1.2.0",
    "gulp-gh-pages": "^0.5.4",
    "gulp-grunt": "^0.5.5",
    "gulp-line-ending-corrector": "^1.0.1",
    "gulp-nsp": "^2.4.2",
    "gulp-plumber": "^1.1.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.5",
    "load-grunt-tasks": "^3.5.2",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "mocha": true
    }
  },
  "nyc": {
    "include": ["lib/**/*.js"],
    "exclude": ["test/**/*.spec.js", "gulpfile.js", "Gruntfile.js"],
    "reporter": ["html", "lcov", "text-summary", "text"],
    "temp-directory": "./coverage/.nyc_output"
  },
  "scripts": {
    "prepublishOnly": "gulp prepublish",
    "test": "nyc --require babel-core/register _mocha test/**/*.spec.js"
  },
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.4"
  }
}
